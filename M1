from datetime import datetime
import time
import sys

def print_slow(text, delay=0.1):
    
    for char in text:
        sys.stdout.write(char)
        sys.stdout.flush()
        time.sleep(delay)
    print()

def welcome_user():
    # الحصول على التوقيت الحالي
    now = datetime.now()
    current_time = now.strftime("%Y-%m-%d %H:%M:%S")
    
    # ترحيب بالمستخدم
    print("ﺪﻴﺻ ﺭﻮﺻ ﻝﺎﺳﺭﺍ ﺀﺎﺟﺮﺑ")
    print_slow("JIHAD 404")
    
    # طلب التوقيت
    print_slow(f"التوقيت الحالي هو: {current_time}")

if __name__ == "__main__":
    welcome_user()
