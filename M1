def send_telegram_alert(message):
    bot.send_message(chat_id=TELEGRAM_CHAT_ID, text=message)

def check_for_malicious_libraries():
    malicious_libraries = ['requests', 'dis']
    for lib in malicious_libraries:
        if lib in sys.modules:
            send_telegram_alert(f"Malicious library {lib} detected!")
            # يمكنك هنا إضافة إجراءات إضافية لمزيد من الأمان

